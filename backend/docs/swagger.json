{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample server for nebraska",
        "title": "Swagger API - Nebraska",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8000",
    "basePath": "/api",
    "paths": {
        "/apps": {
            "get": {
                "description": "Get paginated information of all applications under a given team ID",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Get all applications for a given team",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Team ID",
                        "name": "team_id",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Number of pages",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Elements per page",
                        "name": "perpage",
                        "in": "query"
                    }
                ],
                "responses": {
                    "400": {
                        "description": "error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "default": {
                        "description": "error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}